{
  "rewrites": [
    {
      "source": "/api/booking/healthz",
      "destination": "/api/booking/healthz"
    },
    {
      "source": "/api/autopilot/healthz",
      "destination": "/api/autopilot/healthz"
    },
    {
      "source": "/api/data-rel/healthz",
      "destination": "/api/data-rel/healthz"
    },
    {
      "source": "/api/private-ai/healthz",
      "destination": "/api/private-ai/healthz"
    },
    {
      "source": "/api/revenue/healthz",
      "destination": "/api/revenue/healthz"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
